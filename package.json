{
  "name": "proof-concept-graphql",
  "version": "1.0.0",
  "description": "Proof concept GraphQL node.js",
  "main": "index.js",
  "scripts": {
    "commit": "git-cz",
    "commitmsg": "validate-commit-msg",
    "lint": "eslint --fix .",
    "precommit": "eslint .",
    "test": "jest test/**/*.test.js",
    "start": "node index.js",
    "start:dev": "nodemon index.js"
  },
  "nodemonConfig": {
    "ignore": [
      "node_modules"
    ],
    "nodeArgs": [
      "--inspect=5820"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/andreybejaranobondacom/proof-concept-graphql.git"
  },
  "author": "Andrey Bejarano",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andreybejaranobondacom/proof-concept-graphql/issues"
  },
  "homepage": "https://github.com/andreybejaranobondacom/proof-concept-graphql#readme",
  "dependencies": {
    "apollo-server-express": "^1.3.6",
    "body-parser": "^1.18.3",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "graphql": "^0.13.2",
    "graphql-tools": "^3.0.2",
    "merge-graphql-schemas": "^1.5.1",
    "request": "^2.87.0",
    "request-promise": "^4.2.2",
    "uriparser": "https://github.com/andreybejarano/uriparser.git"
  },
  "devDependencies": {
    "commitizen": "^2.10.0",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jasmine": "^2.10.1",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.1.0",
    "graphql-apollo-errors": "^2.0.3",
    "husky": "^0.14.3",
    "jest": "^23.0.0",
    "nock": "^9.2.6",
    "nodemon": "^1.17.5",
    "validate-commit-msg": "^2.14.0"
  }
}
